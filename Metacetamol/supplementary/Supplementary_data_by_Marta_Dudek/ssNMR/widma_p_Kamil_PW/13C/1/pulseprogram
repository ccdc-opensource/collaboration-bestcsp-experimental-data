# 1 "mc_line 43 file C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp expanding definition part of mc command before ze"
define delay MCWRK
define delay MCREST
"MCWRK = 0.333333*1m"
"MCREST = 10m - 1m"
    dccorr
# 43 "C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp"
1 ze
# 1 "mc_line 43 file C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp expanding definition of mc command after ze"
# 44 "C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp"
# 1 "mc_line 44 file C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp expanding start label for mc command"
2 MCWRK  * 2 do:f2
LBLF0, MCWRK 
  MCREST
# 45 "C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp"

9835472 d19 pl19:f2 \n (p19 ph19^):f2 \n lo to 9835472 times l19

d1 do:f2
# 1 "C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/p15_prot.incl" 1
;p15_prot.incl

; Test the rf on-time and duty cycle
;protect against too long contact time
;Test the rf on-time and duty cycle 
;$COMMENT=protect against too long contact time
;$CLASS=Solids INCL
;$OWNER=nmrsu
1m
if "p15<10.1m" goto Passp15
print "contact time exceeds 10msec limit!"
goto HaltAcqu
Passp15, 1m



# 49 "C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp" 2

			;make sure p15 does not exceed 10 msec	
			;let supervisor change this pulseprogram if 
			;more is needed

			;disable protection file for long acquisition change decoupling power !!! or you risk probe damage
			;if you set the label lacq (ZGOPTNS -Dlacq), the protection is disabled

# 1 "C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/aq_prot.incl" 1
;aq_prot.incl
; Test the rf on-time and duty cycle
;protect against too long acquisition time
;$COMMENT=protect against too long acquisition time
;$CLASS=Solids INCL
;$OWNER=nmrsu
1m
if "aq<50.1m" goto Passaq
print "acquisition time exceeds 50m limit!"
goto HaltAcqu
Passaq, 1m



# 57 "C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp" 2

			;allows max. 50 msec acquisition time, supervisor
			;may change  to max. 1s at less than 5 % duty cycle
			;and reduced decoupling field



983547 d20 pl20:f1 \n (p20 ph20^):f1 \n lo to 983547 times l20

;90 on H
(p3 pl3 ph1):f2 



;cp H (f2) to X (f1)

;jesli zdefiniowane -DRamp to CP z Shape (1,Tang) z F2

(p15 pl1 ph2):f1 (p15:sp0 ph10):f2 

;inaczej bez Shape





1u cpds2:f2 ; decoupling 
  go=2 ph31 ;akwizycja
# 1 "mc_line 85 file C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp expanding mc command in line"
  MCWRK do:f2 wr #0
  MCWRK zd 
  lo to LBLF0 times td0

  MCWRK
# 86 "C:/Bruker/TOPSPIN2.1_6/exp/stan/nmr/lists/pp/user/cp.pp"
HaltAcqu, 1m
exit

ph1=1 3
ph2=0 0 2 2 1 1 3 3 
ph10=0
ph31=0 2 2 0 1 3 3 1

ph19= 0  \n ph20= 0




